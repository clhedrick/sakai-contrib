	<repositories>
		<repository>
			<id>sakai-maven</id>
			<name>Sakai Maven Repo</name>
			<layout>default</layout>
			<url>http://source.sakaiproject.org/maven2</url>
			<snapshots>
				<enabled>false</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>sakai-maven2-snapshots</id>
			<name>Sakai Maven Repo</name>
			<layout>default</layout>
			<url>http://source.sakaiproject.org/maven2-snapshots</url>
			<snapshots>
				<enabled>true</enabled>
			</snapshots>
		</repository>
		<repository>
			<id>ucd-maven</id>
			<url>http://maven2.ucdavis.edu/maven2</url>
		</repository>
		<repository>
			<id>ibiblio</id>
			<url>http://mirrors.ibiblio.org/pub/mirrors/maven2</url>
		</repository>
		<repository>
		    <id>sencha-commercial-release</id>
		    <name>Sencha commercial releases</name>
		    <url>https://maven.sencha.com/repo/commercial-release/</url>
		</repository>
	</repositories>

	<properties>
		<gb2.version>1.9.0-SNAPSHOT</gb2.version>
		<gxt.version>2.2.5-gwt22</gxt.version>
		<gxt3.version>3.0.1</gxt3.version>
		<gwt.version>2.5.0</gwt.version>
		<!-- following fix for bug in gwt 2.3.0 poms (should've been included in those but were not) -->
		<javax.validation.version>1.0.0.GA</javax.validation.version>
		<gb2.commons.logging.version>1.0.4</gb2.commons.logging.version>
		<gb2.jackson.version>1.9.9</gb2.jackson.version> 
		<gb2.jersey.version>1.13</gb2.jersey.version>
	</properties>

	<scm>
	  <developerConnection>scm:svn:https://source.sakaiproject.org/contrib/gradebook2/trunk</developerConnection>
	</scm>
	<profiles>

	    <profile>
		<id>branch</id>
		<activation>
		    <property>
			<name>branchName</name>
		    </property>
		</activation>
		<build>
		    <plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-release-plugin</artifactId>
			<version>2.3</version>
			<configuration>
				<branchBase>https://source.sakaiproject.org/contrib/gradebook2/branches</branchBase>
				<autoVersionSubmodules>true</autoVersionSubmodules>
				<suppressCommitBeforeBranch>true</suppressCommitBeforeBranch>
				<remoteTagging>false</remoteTagging>
				<updateBranchVersions>true</updateBranchVersions>
				<updateWorkingCopyVersions>false</updateWorkingCopyVersions>
				<tagBase>https://source.sakaiproject.org/contrib/gradebook2/tags</tagBase>
			</configuration>
			</plugin>
		    </plugins>
		</build>
	    </profile>

	    <profile>
		<id>tag</id>
		<activation>
		    <property>
			<name>tagName</name>
		    </property>
		</activation>
		<build>
		    <plugins>
			<plugin>
				<groupId>org.apache.maven.plugins</groupId>
			<artifactId>maven-release-plugin</artifactId>
			<version>2.3</version>
			<configuration>
				<autoVersionSubmodules>true</autoVersionSubmodules>
				<suppressCommitBeforeTag>true</suppressCommitBeforeTag>
				<remoteTagging>false</remoteTagging>
				<updateTagVersions>true</updateTagVersions>
				<updateWorkingCopyVersions>false</updateWorkingCopyVersions>
				<tagBase>https://source.sakaiproject.org/contrib/gradebook2/tags</tagBase>
			</configuration>
			</plugin>
		    </plugins>
		</build>
	    </profile>

